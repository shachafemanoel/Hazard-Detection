    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "form-data": "^4.0.0",
    "http-proxy-middleware": "^3.0.5",
    "multer": "^1.4.5-lts.2",
    "ol": "^10.6.1",
