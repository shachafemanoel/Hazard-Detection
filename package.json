{
  "name": "hazard-detection",
  "version": "1.0.0",
  "description": "Hazard Detection Project",
  "main": "server/routes/server.js",
  "type": "module",
  "scripts": {
    "prestart": "npm install --no-audit --no-fund",
    "start": "node server/routes/server.js",
    "dev": "nodemon server/routes/server.js",
    "build": "mkdir -p public/web && cp -r web/* public/web/ && echo 'Web models copied to public directory'",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "stream:check": "node scripts/stream-check.js",
    "live:test": "node public/js/live_test_harness.js",
    "live:report": "node -e \"const results = require('./live-metrics.json'); console.log(JSON.stringify(results, null, 2));\""
  },
